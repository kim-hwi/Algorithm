%!
%% x_pos, y_pos
/mark{
    moveto
    3 5 rlineto
    3 -5 rlineto
    -3 -5 rlineto
    closepath
    2 setlinewidth
}def

/mark2{
    moveto
    3 5 rlineto
    3 -5 rlineto
    -3 -5 rlineto
    closepath
    2 setlinewidth
}def

/card{
    moveto
    255.1165 0 rlineto
    0 141.1731 rlineto
    -255.1165 0 rlineto
    closepath
    1 setlinewidth
    0.0 0.0 0 setrgbcolor
    0 0 moveto
    20 100 moveto
    /Times-Roman findfont
    10 scalefont
    setfont
    (Postscript Programmer) show
    20 80 moveto
    (KIM HWI) show
    20 70 moveto
    100 70 lineto
    20 60 moveto
    (HP - 010 9019 0879) show
    stroke
    185 80 60 0 360 arc stroke

    1 1 20 { /i exch def
        i 10 gt{ 
            200 i 3 mul sub -20 i 5 mul add mark
            230 i 3 mul sub -20 i 5 mul add mark
            235 i 3 mul sub 25 i 5 mul add mark
            260 i 3 mul sub 25 i 5 mul add mark}
        {
            140 i 3 mul add 80 i 5 mul add mark2
            195 i 3 mul add 30 i 5 mul add mark2
        } ifelse
        i 5 mod 1 eq{ 0.2 i 20 div i 20 div setrgbcolor}
        {0.8 0.8 0 setrgbcolor}
        ifelse
        stroke
    }for

}def


10 10 card
/cardprint{
    0 180 translate
    10 10 card
    0 180 translate
    10 10 card
    300 0 translate
    10 10 card
    0 -180 translate
    10 10 card
    0 -180 translate
    10 10 card

} def
cardprint
showpage